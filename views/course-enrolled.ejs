<body>

    <div>
        <h1 style="text-align: center;">My Courses</h1>
    </div>
    <hr>
    <div style="margin-left: 350px; margin-right: 350px;">
        <h2>Courses</h2>
        <% if(courses!=null) { %>
            <table id="enrolled" class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Course Code</th>
                        <th>Course Instructor</th>
                        <th>Prerequisite</th>
                        <th>Course Page </th>
                    </tr>
                </thead>
                <tbody>
                    <% courses.forEach(function(item){ %>
                        <tr>
                            <td>
                                <%= item.name%>
                            </td>
                            <td>
                                <%= item.code%>
                            </td>
                            <td>
                                <%= item.instructor%>
                            </td>
                            <td>
                                <%= item.prerequisite%>
                            </td>
                            <td>
                                <a href='singlecoursepage?code=<%=item.code%>'>Open Course Page</a>
                            </td>


                        </tr>
                        <% }) %>
                </tbody>
            </table>
            <% } %>
    </div>

</body>
<script>
    $(document).ready(function () {
        $('#enrolled').DataTable();
    });
</script>

</html>